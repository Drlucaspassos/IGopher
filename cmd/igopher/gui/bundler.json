{
    "app_name": "IGopher",
    "icon_path_darwin": "resources/favicon.icns",
    "icon_path_linux": "resources/favicon.png",
    "icon_path_windows": "resources/favicon.ico",
    "environments": [{
            "arch": "amd64",
            "os": "darwin"
        },
        {
            "arch": "amd64",
            "os": "linux"
        },
        {
            "arch": "amd64",
            "os": "windows",
            "env": {
                "CC": "x86_64-w64-mingw32-gcc",
                "CXX": "x86_64-w64-mingw32-g++",
                "CGO_ENABLED": "1"
            }
        }
    ]
}