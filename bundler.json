{
    "app_name": "IGopher",
    "icon_path_darwin": "cmd/igopher/gui/resources/favicon.icns",
    "icon_path_linux": "cmd/igopher/gui/resources/favicon.png",
    "icon_path_windows": "cmd/igopher/gui/resources/favicon.ico",
    "input_path": "cmd/igopher/gui/",
    "environments": [
        {
            "arch": "amd64",
            "os": "darwin"
